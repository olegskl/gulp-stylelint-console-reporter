{
  "name": "gulp-stylelint-console-reporter",
  "version": "0.1.0",
  "description": "gulp-stylelint reporter to display stylelint results in console",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint {test,src}/**/*.js",
    "test": "npm run lint && babel-tape-runner test/*.spec.js",
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olegskl/gulp-stylelint-console-reporter.git"
  },
  "keywords": [
    "stylelint",
    "reporter",
    "console",
    "postcss",
    "css"
  ],
  "author": "Oleg Sklyanchuk <sklyanchuk@gmail.com> (http://olegskl.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olegskl/gulp-stylelint-console-reporter/issues"
  },
  "homepage": "https://github.com/olegskl/gulp-stylelint-console-reporter",
  "dependencies": {
    "gulp-util": "^3.0.7"
  },
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-tape-runner": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-meetic": "^0.1.2",
    "sinon": "^1.17.3",
    "tape": "^4.4.0"
  }
}
